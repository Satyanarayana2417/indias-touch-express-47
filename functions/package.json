{
  "name": "venkat-express-functions",
  "description": "Firebase Cloud Functions for Venkat Express",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "18"
  },
  "main": "lib/index.js",
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "firebase-admin": "^12.1.0",
    "firebase-functions": "^5.0.0",
    "form-data": "^4.0.4",
    "sharp": "^0.33.0",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/validator": "^13.11.0",
    "typescript": "^4.9.0"
  },
  "private": true
}
